function doAdvSearch(vendormod)
{
	var ajaxRequest;  // The variable that makes Ajax possible!
	
	try
	{
		// Opera 8.0+, Firefox, Safari
		ajaxRequest = new XMLHttpRequest();
	} 
	catch (e)
	{
		// Internet Explorer Browsers
		try
		{
			ajaxRequest = new ActiveXObject("Msxml2.XMLHTTP");
		} 
		catch (e) 
		{
			try
			{
				ajaxRequest = new ActiveXObject("Microsoft.XMLHTTP");
			} 
			catch (e)
			{
				// Something went wrong
				alert("Your browser broke!");
				return false;
			}
		}
	}
	
	// Create a function that will receive data sent from the server
	ajaxRequest.onreadystatechange = function()
	{
		if(ajaxRequest.readyState == 4)
		{
			var ajaxDisplay = document.getElementById('divItems');
			document.getElementById('divLodImg').style.display='none'; 
			document.getElementById('divItems').style.display='block';
			ajaxDisplay.innerHTML = ajaxRequest.responseText;
		}
	}
	var vandor="";
	if(vendormod == "yes")
	{
		vendor = "&vendor="+document.getElementById('cmbvendorsearch').value;
	}
	else
	{
		vendor = "";
	}
	var modval="search";
	var queryString="?typeid="+document.getElementById('cmbitemtype').value+"&search="+document.getElementById('txtSearch').value+"&size="+document.getElementById('cmbSize').value+"&tax="+document.getElementById('cmbTax').value+"&mod="+modval+vendor; 
			
			ajaxRequest.open("GET", "ajax_advsearch.php" + queryString , true);
			
			ajaxRequest.send(null); 
}

